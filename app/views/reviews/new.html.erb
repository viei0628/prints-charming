<div class="container">
  <div class="row justify-content-center py-5 ">
    <div class="col-md-6">
      <h2>Add A Review</h2>
      <%= simple_form_for [ @printer, @review ] do |f| %>
        <%= f.input :text %>
        <%= f.button :submit, "Add Review", class: "btn-flat" %>
      <% end %>
    </div>
  </div>
</div>
