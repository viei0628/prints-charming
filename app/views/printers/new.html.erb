<%= render "shared/navbar" %>
<div class="container">
  <div class="row justify-content-center py-5">
    <div class="col-md-6">
      <h2>Add A Printer</h2>
      <%= simple_form_for @printer do |f| %>
        <%= f.input :model %>
        <%= f.input :price %>
        <%= f.input :photo, as: :file %>
        <%= f.button :submit, "Add Printer", class: "btn btn-flat" %>
      <% end %>
    </div>
  </div>
</div>
