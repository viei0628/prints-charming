<div class="container">
  <div class="row justify-content-center py-5">
    <div class="form-content col-sm-12 col-md-8 col-lg-10 my-5 p-5">
      <%= render "shared/custom-navbar" %>
      <h2 class="my-3">Book <%= @printer.model %> </h2>
        <%= simple_form_for [@printer, @booking] do |f| %>
        <%= f.input :meeting_time %>
        <%= f.button :submit, "Book Printer", class: "btn-flat my-3" %>
      <% end %>
    </div>
  </div>
</div>
